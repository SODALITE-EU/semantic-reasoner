SELECT ?m ?time ?user ?dsl ?name {
		?m a soda:AbstractApplicationDeployment .
		?m soda:createdAt ?time .
		?m soda:createdBy ?user .
		?m soda:hasDSL ?dsl .
		?m soda:hasName ?name .
		
		?m DUL:isSettingFor ?n .
        
		FILTER NOT EXISTS
		{ 
			GRAPH ?g { ?n ?p ?o } 
		}
    
		?n soda:hasContext ?c
} group by ?m ?time ?user ?dsl